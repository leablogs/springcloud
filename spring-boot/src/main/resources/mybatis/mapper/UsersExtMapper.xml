<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.leablogs.user.mapper.UsersExtMapper">
    <select id="selectById" resultType="com.leablogs.user.entity.UsersExt">
        select *
        from ${prefix}users_ext
        where user_id = #{user_id}
    </select>
</mapper>
